@model InternetApp.Models.UserDetailsModel

@{
    ViewBag.Title = "Details";
}

<h2>@Html.DisplayFor(model => model.UserProfiles.UserName)'s Profile</h2>

<p>
    Name: @Html.DisplayFor(model => model.UserProfiles.FirstName) @Html.DisplayFor(model => model.UserProfiles.LastName)<br />
    E-mail: Name: @Html.DisplayFor(model => model.UserProfiles.Email)
</p>

<p>
    Projects @Html.DisplayFor(model => model.UserProfiles.FirstName) owns:
    <ul>
   @foreach (var p in Model.Projects)
   {
       <li>@Html.ActionLink(@p.Name, "../Project/Details/" + @p.ProjectId)</li>
   }
   </ul>
</p>
