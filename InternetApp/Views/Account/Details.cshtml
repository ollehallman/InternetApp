@model InternetApp.Models.UserDetailsModel

@{
    ViewBag.Title = "Details";
}

<h2>@ViewBag.userIdent Profile</h2>

<!-- @Html.DisplayFor(model => model.UserProfiles.UserId) -->

<p>
    Projects user @ViewBag.userIdent owns:
    <ul>
   @foreach (var p in Model.Projects)
   {
       <li>@Html.ActionLink(@p.Name, "../Project/Details/" + @p.ProjectId)</li>
   }
   </ul>
</p>
